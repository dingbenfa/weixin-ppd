<!--pages/user/suggestions/suggestions.wxml-->
<view class='m-suggestions-container'>
    <view class='m-suggestions-name m-font-grey'>请选择反馈内容</view>
    <view class='m-suggestions-box'>
        <view class='m-suggestions-items'>
            <text>无法打开小程序</text>
            <view class='m-next-icon m-font-grey'>></view>
        </view>
        <view class='m-suggestions-items'>
            <text>小程序闪退</text>
            <view class='m-next-icon m-font-grey'>></view>
        </view>
        <view class='m-suggestions-items'>
            <text>卡顿</text>
            <view class='m-next-icon m-font-grey'>></view>
        </view>
         <view class='m-suggestions-items'>
            <text>黑屏白屏</text>
            <view class='m-next-icon m-font-grey'>></view>
        </view>
        <view class='m-suggestions-items'>
            <text>死机</text>
            <view class='m-next-icon m-font-grey'>></view>
        </view>
        <view class='m-suggestions-items'>
            <text>界面错位退</text>
            <view class='m-next-icon m-font-grey'>></view>
        </view>
         <view class='m-suggestions-items'>
            <text>界面加载慢</text>
            <view class='m-next-icon m-font-grey'>></view>
        </view>
        <view class='m-suggestions-items' bindtap='handleOtherAbnormal'>
            <text>其他异常</text>
            <view class='m-next-icon m-font-grey'>></view>
        </view>
        <view class='m-suggestions-items'>
            <text>意见与建议</text>
            <view class='m-next-icon m-font-grey'>></view>
        </view>
    </view>
</view>